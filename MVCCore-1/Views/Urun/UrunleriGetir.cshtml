@model IEnumerable<Urun>
<a asp-action="UrunEkle" class="btn btn-primary">Ürün ekle</a>
<table class="table table-striped">
    @* <thead>
        <tr>Id</tr>
        <tr>Ad</tr>
        <tr>Fiyat</tr>
    </thead> *@
    @foreach(var urun in Model){
        <tr>
            <td>@urun.UrunId</td>
            <td>@urun.UrunAdi</td>
            <td>@urun.Fiyat</td>
        </tr>
    }
</table>