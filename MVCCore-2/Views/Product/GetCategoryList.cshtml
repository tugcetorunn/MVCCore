@model IEnumerable<Category>

@{
    ViewData["Title"] = "Category List";
}

<h2>Category List</h2>
<table>
    <thead>
        <tr>
            <th>Category Id</th>
            <th>Name</th>
        </tr>
    </thead>
    @foreach (var item in Model)
    {
        <tr>
            <td>@item.CategoryId</td>
            <td>@item.Name</td>
        </tr>
    }
</table>
